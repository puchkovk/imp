{
    "name": "theimp/theimp",
    "description": "Some tools assembled into library",
    "license": "MIT",
    "require-dev": {
        "phpstan/phpstan": "^0.12.36",
        "phpunit/phpunit": "^9.3"
    },
    "authors": [
        {
            "name": "Konstantin Puchkov",
            "email": "puchkovk+imp@ya.ru"
        }
    ],
    "require": {
        "league/container": "^3.3",
        "laminas/laminas-diactoros": "^2.3",
        "laminas/laminas-httphandlerrunner": "^1.2",
        "league/route": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "TheImp\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheImpTest\\": "./tests/"
        }
    },
    "scripts": {
        "stan" : [
            "./vendor/bin/phpstan analyse . --memory-limit=1000M --level max -c tests/phpstan.neon"
        ],
        "unit": [
            "./vendor/bin/phpunit -c tests/phpunit.xml"
        ]
    }
}
